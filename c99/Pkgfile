# Description: Shell wrapper script to ANSI (1999) C compiler
# URL:         https://zeppe-lin.github.io/

name=c99
version=0.1
release=1
source="c99.sh c99.1"

build() {
	# Thanks to NetBSD for manual page.
	install -m 0755 -D $SRC/c99.sh $PKG/usr/bin/c99
	install -m 0644 -D $SRC/c99.1  $PKG/usr/share/man/man1/c99.1
}
